<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.*?>

<Pane prefHeight="300.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="gui.AddBookController">
  <children>
    <GridPane layoutX="120.0" layoutY="30.0" prefWidth="260.0">
      <children>
        <TextField prefWidth="200.0" promptText="%title" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <TextField prefWidth="200.0" promptText="%author" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <TextField prefWidth="200.0" promptText="%publisher" GridPane.columnIndex="0" GridPane.rowIndex="3" />
        <TextField prefWidth="200.0" promptText="%year" GridPane.columnIndex="0" GridPane.rowIndex="4" />
        <TextField prefWidth="200.0" promptText="%edition" GridPane.columnIndex="0" GridPane.rowIndex="5" />
        <SplitMenuButton id="userType" mnemonicParsing="false" prefWidth="260.0" text="%group" GridPane.columnIndex="0" GridPane.rowIndex="6">
          <items>
            <MenuItem mnemonicParsing="false" text="%textBook" />
            <MenuItem mnemonicParsing="false" text="%otherBook" />
          </items>
        </SplitMenuButton>
        <GridPane GridPane.columnIndex="0" GridPane.rowIndex="7">
          <children>
            <Button id="okBtn" contentDisplay="LEFT" defaultButton="true" mnemonicParsing="false" text="%ok" textAlignment="LEFT" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
              <GridPane.margin>
                <Insets left="2.0" />
              </GridPane.margin>
            </Button>
            <Button id="cancelBtn" cancelButton="true" mnemonicParsing="false" text="%cancel" GridPane.columnIndex="0" GridPane.halignment="RIGHT" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
              <GridPane.margin>
                <Insets right="2.0" />
              </GridPane.margin>
            </Button>
          </children>
          <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          </rowConstraints>
        </GridPane>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="%addBook" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.vgrow="ALWAYS">
          <font>
            <Font name="System Bold" size="20.0" />
          </font>
        </Text>
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </children>
</Pane>
